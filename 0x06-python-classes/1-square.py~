#!/usr/bin/python3
# 1-square.py

class Square:
    """
    Defines a square object.
    """
    def __init__(self, size):
    self.size = size

    def size(self):
    return self.__size

if __name__ == "__main__":
    Square = __import__('1-square').Square
    my_square = Square(3)
    print(type(my_square))
    print(my_square.__dict__)

    try:
        print(my_square.size())
    except Exception as e:
        print(e)

    try:
        print(my_square.size())
    except Exception as e:
        print(e)
